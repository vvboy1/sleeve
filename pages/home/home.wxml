      indicator-dots
      indicator-active-color="#157641"
      autoplay
      circular
    >
      <block wx:for="{{bannerB.items}}" >
        <swiper-item>
          <image class="swiper" src="{{item.img}}"></image>
        </swiper-item>
      </block>
